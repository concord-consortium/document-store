{
  "name": "document-store",
  "scripts": {
  },
  "env": {
    "CODAP_DEFAULT_URL": {
      "required": true
    },
    "DEVISE_PEPPER": {
      "generator": "secret"
    },
    "DEVISE_SECRET_KEY": {
      "generator": "secret"
    },
    "LANG": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "generator": "secret"
    }
  },
  "formation": {
    "worker": {
      "quantity": 1
    },
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ]
}
